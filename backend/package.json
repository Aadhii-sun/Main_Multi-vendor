{
  "name": "backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node utils/connectionTester.js",
    "test:connections": "node utils/connectionTester.js",
    "test:api": "curl -s http://localhost:5000/api/test/config | jq",
    "test:mongodb": "node -e \"require('./utils/connectionTester').testMongoDB().then(r => console.log('MongoDB:', r.message)).catch(console.error)\"",
    "test:stripe": "node -e \"require('./utils/connectionTester').testStripe().then(r => console.log('Stripe:', r.message)).catch(console.error)\"",
    "test:email": "node -e \"require('./utils/connectionTester').testEmail().then(r => console.log('Email:', r.message)).catch(console.error)\"",
    "seed:admin": "node scripts/seedAdmin.js",
    "debug:admin": "node scripts/checkAdmin.js"
  },
  "keywords": [
    "ecommerce",
    "api",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "description": "E-commerce backend API with user authentication, product management, and payment processing",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-random-string": "^5.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.2",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.10",
    "stripe": "^19.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
